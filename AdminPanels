local flingBtn = MakeButton(contents["Misc"], UDim2.new(0.9, 0, 0, 38), UDim2.new(0, 0, 0, 100), "üåÄ Fling Yourself", function()
    local char = player.Character
    if not char then return end

    local hrp = char:FindFirstChild("HumanoidRootPart")
    if not hrp then return end

    -- Add rotational BodyAngularVelocity
    local bav = Instance.new("BodyAngularVelocity")
    bav.AngularVelocity = Vector3.new(0, 50000, 0)
    bav.MaxTorque = Vector3.new(0, math.huge, 0)
    bav.P = 1250
    bav.Name = "Flinger"
    bav.Parent = hrp

    -- Optional: Add velocity to push outward
    local bv = Instance.new("BodyVelocity")
    bv.Velocity = Vector3.new(100, 100, 100)
    bv.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
    bv.P = 1250
    bv.Name = "FlingPush"
    bv.Parent = hrp

    -- Cleanup after 2 seconds
    task.delay(2, function()
        if bav and bav.Parent then bav:Destroy() end
        if bv and bv.Parent then bv:Destroy() end
    end)
end)
