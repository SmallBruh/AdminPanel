-- Infinite Jump Variables
local infJumpEnabled = false
local infJumpConn

-- UI Section
local funSection = contents["Fun"]

-- Container Frame
local jumpSection = Instance.new("Frame")
jumpSection.Size = UDim2.new(1, -20, 0, 60)
jumpSection.BackgroundTransparency = 1
